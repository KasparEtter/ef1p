{
    "blanks-around-headings": {
        "lines_below": 0
    },
    "blanks-around-lists": false,
    "fenced-code-language": false,
    "first-header-h1": false,
    "first-line-h1": false,
    "line-length": false,
    "no-bare-urls": false,
    "no-duplicate-heading": {
        "siblings_only": true
    },
    "no-inline-html": {
        "allowed_elements": [
            "a",
            "i",
            "details",
            "div",
            "figure",
            "figcaption",
            "img",
            "span",
            "summary"
        ]
    },
    "no-multiple-blanks": false,
    "no-trailing-punctuation": {
        "punctuation": ",;"
    },
    "proper-names": {
        "names": [
            "Ethereum"
        ]
    },
    "single-h1": {
        "front_matter_title": ""
    }
}
